package ocdSim;

import processing.core.*;

public class OverworldFoodFactory implements OverworldObjectFactory {

	@Override
	public OverworldObject generate(PVector pos, PVector dim, float parentTranslationX, float parentTranslationZ,
			float checkRadius, PVector quaternion) {
		
	    return new OverworldFood(pos, dim, parentTranslationX, parentTranslationZ, checkRadius, quaternion, new FoodObjectData(OCDSimulator.getRandomFoodType()));
	  
	}

}
